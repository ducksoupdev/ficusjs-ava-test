{
    "name": "ficusjs-ava-test",
    "version": "1.1.0",
    "description": "Example unit testing web components with ava and jsdom",
    "engines": {
        "node": ">=14"
    },
    "engineStrict": true,
    "scripts": {
        "test": "ava",
        "test:coverage": "c8 --all --include=src/**/*.mjs --reporter=lcov --reporter=text-summary ava test/**/*.spec.mjs --verbose"
    },
    "author": "Matt Levy",
    "license": "MIT",
    "devDependencies": {
        "@ficusjs/renderers": "3.1.0",
        "@ficusjs/testing": "1.0.0",
        "ava": "3.15.0",
        "c8": "7.7.1",
        "ficusjs": "3.2.3",
        "standard": "16.0.3"
    }
}
